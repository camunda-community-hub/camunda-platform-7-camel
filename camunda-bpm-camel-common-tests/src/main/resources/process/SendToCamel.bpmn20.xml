  <process id="sendToCamelProcess" isExecutable="true">
    <startEvent id="start">
      <outgoing>flow1</outgoing>
    </startEvent>
    <sequenceFlow id="flow1" sourceRef="start" targetRef="sendToCamelOneVariableServiceTask"/>
    <serviceTask id="sendToCamelOneVariableServiceTask" activiti:expression="${camel.sendTo(execution, 'direct:sendToCamelServiceTask', 'var1')}" activiti:resultVariableName="result1" name="Send to Camel one variable">
      <incoming>flow1</incoming>
      <outgoing>flow2</outgoing>
    </serviceTask>
    <sequenceFlow id="flow2" sourceRef="sendToCamelOneVariableServiceTask" targetRef="logProcessVariableServiceTask"/>
    <serviceTask id="logProcessVariableServiceTask" activiti:expression="${log.debug(var1)}" name="Log process&#xA;variable">
      <incoming>flow2</incoming>
      <outgoing>SequenceFlow_4</outgoing>
    </serviceTask>
    <serviceTask id="sendToCamelAllProcessVariableServiceTask" activiti:expression="${camel.sendTo(execution, 'direct:sendToCamelServiceTask', processVariables)}" activiti:resultVariable="result2" name="Send to Camel all process variables">
      <incoming>SequenceFlow_4</incoming>
      <outgoing>SequenceFlow_3</outgoing>
    </serviceTask>
    <endEvent id="end">
      <incoming>SequenceFlow_3</incoming>
    </endEvent>
    <sequenceFlow id="SequenceFlow_3" name="" sourceRef="sendToCamelAllProcessVariableServiceTask" targetRef="end"/>
    <sequenceFlow id="SequenceFlow_4" name="" sourceRef="logProcessVariableServiceTask" targetRef="sendToCamelAllProcessVariableServiceTask"/>
  </process>
